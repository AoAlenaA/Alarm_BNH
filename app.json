{
  "expo": {
    "name": "Alarm_BNH",
    "slug": "Alarm_BNH",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/icon.png",
    "scheme": "myapp",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true,
      "infoPlist": {
        "NSMotionUsageDescription": "Приложению требуется доступ к данным шагомера для отслеживания ваших шагов."
      }
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "permissions": ["ACTIVITY_RECOGNITION"],
      "package": "com.ild4vid.Alarm_BNH"
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      [
        "expo-notifications",
        {
          "color": "#ffffff",
          "defaultChannel": "default",
          "sounds": [
            "./assets/sounds/sound_1.mp3",
            "./assets/sounds/sound_2.mp3",
            "./assets/sounds/sound_3.mp3",
            "./assets/sounds/sound_4.mp3",
            "./assets/sounds/sound_5.mp3",
            "./assets/sounds/sound_6.mp3",
            "./assets/sounds/sound_7.mp3",
            "./assets/sounds/sound_8.mp3",
            "./assets/sounds/sound_9.mp3",
            "./assets/sounds/sound_10.mp3",
            "./assets/sounds/sound_11.mp3",
            "./assets/sounds/sound_12.mp3",
            "./assets/sounds/sound_13.mp3",
            "./assets/sounds/sound_14.mp3",
            "./assets/sounds/sound_15.mp3",
            "./assets/sounds/sound_16.mp3",
            "./assets/sounds/sound_17.mp3",
            "./assets/sounds/sound_18.mp3",
            "./assets/sounds/sound_19.mp3",
            "./assets/sounds/sound_20.mp3"
          ]
        }
      ],
      "expo-router",
      [
        "expo-splash-screen",
        {
          "image": "./assets/images/splash-icon.png",
          "imageWidth": 200,
          "resizeMode": "contain",
          "backgroundColor": "#ffffff"
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {
        "origin": false
      },
      "eas": {
        "projectId": "6c47a5cf-75be-4406-9b10-d2ae7fdb4d23"
      }
    }
  }
}